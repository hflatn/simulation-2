create table userbase (
id serial primary key,
username varchar(40),
password varchar(40)
)

create table listing (
 ID SERIAL,
user_id INT,
name varchar(40),
description varchar(40),
address varchar(40),
city varchar(40),
state varchar(40),
zip varchar(40),
imgurl varchar(200),
loan_amount varchar(40),
monthly_mortgage varchar(40),
desired_rent varchar(40)
)